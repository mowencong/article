.aricalLsit{
  margin:0;
  padding:0;
  box-sizing:border-box;
}
.banana{
  width:100%;
  height:240px;
  position:relative;
  margin-bottom:20px;
}
.boxList{
  margin-bottom:10px;
  position:relative;
}
.lunbo_title{
  position:absolute;
  bottom:0;
  left:0;
  right:0;
  font-size:24px;
  height:84px;
  line-height:28px;
  color:#fff;
  background: #000;
  width:100%;
  opacity:0.6;
}
.banana .mwc{
  height:240px;
}
.slide-image{
  width:100%;
  
}
.imagesBox{
  box-sizing:border-box;
  width:100%;
  margin:0 5%;
}
.imagesBox .img{
  width: 90%;
  border:1px solid #fff;
  border-radius: 10px;
}
.title{
  position:absolute;
  bottom:0;
  left:0;
  right:0;
  font-size:24px;
  height:64px;
  line-height:28px;
  color:#fff;
  background: #000;
  width:90%;
  opacity:0.6;
  margin:0 5%;
  border:1px solid #fff;
  /* border-radius: 10px; */
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
}